@{
    Layout = "_Layout";
}
<div class="container" id="client-container">
    <h1>@Model.GetName()</h1>
    <div class="row titles">
        <div class="col-md-5">
            <h4>Stylist Name</h4>
        </div>
        <div class="col-md-3">
            <h4>Details</h4>
        </div>
    </div>
    <div class="row data">
        <div class="col-md-5">
            <p class="name">@Model.FindStylist().GetName()</p>
        </div>
        <div class="col-md-5">
            <p class="name">@Model.FindStylist().GetDetails()</p>
        </div>
    </div>
