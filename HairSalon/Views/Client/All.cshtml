@{
    Layout = "_Layout";
}
<div class="container" id="client-container">
    <h1>All Clients</h1>
    <div class="row titles">
        <div class="col-md-5">
            <h4>Client Name</h4>
        </div>
        <div class="col-md-3">
            <h4>Stylist Id</h4>
        </div>
    </div>
    @foreach (var client in Model)
    {
        <div class="row data">
            <div class="col-md-5">
                <p class="name">@client.GetName()</p>
            </div>
            <div class="col-md-3">
                <p>@client.GetStylistId()</p>            
            </div>
        </div>
    }
</div>
