@{
    Layout = "_Layout";
}
<div class="container" id="client-container">
    <div class="row titles">
        <div class="col-md-5">
            <h1>All Clients</h1>
        </div>
    </div>
    @foreach (var client in Model)
    {
        <div class="row data">
            <div class="col-md-5">
                <h3 class="name"><a href="/client/all/@client.GetId()/details">@client.GetName()</a></h3>
            </div>
        </div>
    }
</div>
