@{
    Layout = "_Layout";
}
<div class="container" id="stylist-container">
    <h1>Stylists who specialize in: @Model.Description</h1>
    <a class="btn btn-success" href="/specialty/all/@Model.Id/update">Update Specialty</a>
    <div class="row titles">
        <div class="col-md-5">
            <h4>Stylists</h4>
        </div>
    </div>
    @foreach (var stylist in Model.GetStylists())
    {
    <div class="row data">
        <div class="col-md-5">
            <p class="name">@stylist.GetName()</p>
        </div>
    </div>
    }
</div>
