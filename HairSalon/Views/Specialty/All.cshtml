@{
    Layout = "_Layout";
}
<div class='container' id="stylist-container">
    <h1>All Specialties</h1>
        <div class="row titles">
            <div class="col-md-5">
                <h3>Specialty</h3>
            </div>
        </div>
        @foreach (var specialty in Model)
        {
        <div class="row data">
            <div class="col-md-5">
                <h4 class="name"><a href="/specialty/all/@specialty.Id/details">@specialty.Description</a></h4>
            </div>
        </div>
        }
</div>
